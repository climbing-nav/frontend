# API Configuration 
VITE_API_URL=your_backend_api_start_point_url_here

# Social Login Configuration
# Google OAuth Client ID (from Google Cloud Console)
VITE_GOOGLE_CLIENT_ID=your_google_client_id_here

# Kakao OAuth Configuration
# Kakao REST API Key (from Kakao Developers - for OAuth login)
VITE_KAKAO_REST_API_KEY=your_kakao_rest_api_key_here

# Kakao OAuth Redirect URI (default: {YOUR_DOMAIN}/auth/kakao/callback)
VITE_KAKAO_REDIRECT_URI=your_kakao_redirect_url_here

# Kakao Map API (for map integration)
REACT_APP_KAKAO_API_KEY=your_kakao_map_api_key_here

# Legacy (deprecated - can be removed if not used)
VITE_KAKAO_APP_KEY=your_kakao_app_key_here

# Environment
NODE_ENV=development